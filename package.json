{
    "name": "mini-chat-ollama",
    "version": "1.0.0",
    "description": "Full-stack chat application with multi-provider AI support (Ollama, OpenRouter, Groq, Anthropic)",
    "main": "index.js",
    "scripts": {
        "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
        "dev:backend": "cd backend && npm run dev",
        "dev:frontend": "cd frontend && npm run dev",
        "start:backend": "cd backend && npm start",
        "start:frontend": "cd frontend && npm start",
        "build:frontend": "cd frontend && npm run build"
    },
    "keywords": [
        "chat",
        "ai",
        "ollama",
        "openrouter",
        "groq",
        "anthropic",
        "nextjs",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/mini-chat-ollama"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "framer-motion": "^12.27.1",
        "lucide-react": "^0.562.0"
    }
}
